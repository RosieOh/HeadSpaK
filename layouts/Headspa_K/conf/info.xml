<?xml version="1.0" encoding="UTF-8"?>
<layout version="0.2">
	<title xml:lang="ko">SIC</title>
	<title xml:lang="en">SIC</title>
	<title xml:lang="jp">SIC</title>
	<title xml:lang="zh-CN">SIC</title>
	<description xml:lang="ko"></description>
	<description xml:lang="en"></description>
	<description xml:lang="jp"></description>
	<description xml:lang="zh-CN"></description>
	<version>v1.0.0</version>
	<date>2020-06-11</date>
	<author email_address="onplus_cpo@naver.com" link="">
		<name xml:lang="ko">ONPLUS</name>
		<name xml:lang="en">ONPLUS</name>
		<name xml:lang="jp">ONPLUS</name>
		<name xml:lang="zh-CN">ONPLUS</name>
	</author>
	
	<extra_vars>
		<!-- basic -->
		<group>
			<title xml:lang="ko">기본 세팅</title>
			<title xml:lang="en">General setting</title>

			<var name="layout_type" type="select" default="main">
				<title xml:lang="ko">레이아웃 타입</title>
				<title xml:lang="en">Layout Type</title>
				<description xml:lang="ko">메인/서브 타입을 선택하세요.</description>
				<description xml:lang="en">Select layout type within main/sub</description>

				<options value="main">
					<title xml:lang="ko">메인 페이지 타입</title>
					<title xml:lang="en">Main Type</title>
				</options>
				<options value="sub">
					<title xml:lang="ko">서브 페이지 타입</title>
					<title xml:lang="en">Sub Type</title>
				</options>
			</var>

			<var name="header_logo" type="image">
				<title xml:lang="ko">헤더 로고</title>
				<title xml:lang="en">Header logo</title>
				<description xml:lang="ko">헤더에 적용할 로고 이미지를 첨부하세요. (jpg, png, gif 허용. 이미지 높이 50픽셀로.)</description>
				<description xml:lang="en">Input logo image to applicate in header area. (jpg, png, gif. image height 50 pixel.)</description>
			</var>

			<var name="header_text" type="text">
				<title xml:lang="ko">로고 텍스트</title>
				<title xml:lang="en">Logo text</title>
				<description xml:lang="ko">헤더 로고에 대체하여 사용할 텍스트를 입력하세요.</description>
				<description xml:lang="en">Input logo text to position in header.</description>
			</var>

			<var name="logo_text_color" type="text" value="#888">
				<title xml:lang="ko">로고 텍스트 컬러</title>
				<title xml:lang="en">Header text color</title>
				<description xml:lang="ko">헤더 로고 텍스트에 사용할 컬러값을 입력하세요. (Hex 값 입력 - ex:#888)</description>
				<description xml:lang="en">Input color value for header logo text. (Hex value - ex:#888)</description>
			</var>

			<var name="header_logo_link" type="text">
				<title xml:lang="ko">로고 링크</title>
				<title xml:lang="en">Logo link</title>
				<description xml:lang="ko">헤더 로고에 연결할 url을 입력하세요.</description>
				<description xml:lang="en">Input url to link with header logo</description>
			</var>

			<var name="member_use" type="select" default="Y">
				<title xml:lang="ko">회원 메뉴 사용</title>
				<title xml:lang="en">Member menu use</title>
				<description xml:lang="ko">회원 메뉴 사용 여부를 선택하세요</description>
				<description xml:lang="en">Decide use of member menu</description>

				<options value="Y">
					<title xml:lang="ko">사용함</title>
					<title xml:lang="en">Use</title>
				</options>

				<options value="N">
					<title xml:lang="ko">사용하지 않음</title>
					<title xml:lang="en">Not use</title>
				</options>
			</var>

			<var name="profile_img" type="image">
				<title xml:lang="ko">프로필 이미지</title>
				<title xml:lang="en">Profile image</title>
				<description xml:lang="ko">회원 메뉴 박스 프로필에 사용할 이미지를 적용합니다. (150*150픽셀 이미지 권장.)</description>
				<description xml:lang="en">Apply profile image for member menu box profile (150*150pixel image recommended.)</description>
			</var>

			<var name="profile_desc" type="text">
				<title xml:lang="ko">프로필 캡션</title>
				<title xml:lang="en">Profile caption</title>
				<description xml:lang="ko">프로필 이미지 하단에 사용할 텍스트를 입력하세요.</description>
				<description xml:lang="en">Input text which will be positioned under the profile image.</description>
			</var>

			<var name="header_background" type="text">
				<title xml:lang="ko">헤더 배경색</title>
				<title xml:lang="en">Header background color</title>
				<description xml:lang="ko">헤더 배경에 사용할 컬러값을 입력하세요. (Hex 값 입력 - ex:#888)</description>
				<description xml:lang="en">Input color value for header background. (Hex value - ex:#888)</description>
			</var>

			<var name="menu_color" type="text">
				<title xml:lang="ko">메뉴 색상</title>
				<title xml:lang="en">Menu color</title>
				<description xml:lang="ko">헤더 메뉴에 사용할 컬러값을 입력하세요. (Hex 값 입력 - ex:#888)</description>
				<description xml:lang="en">Input color value for header menu. (Hex value - ex:#888)</description>
			</var>

			<var name="point_color" type="text">
				<title xml:lang="ko">포인트 컬러</title>
				<title xml:lang="en">Point color</title>

				<description xml:lang="ko">버튼 기본 배경색 및 메뉴 마우스 오버 시 색상을 결정합니다. (Hex값 입력 - ex: #888)</description>
				<description xml:lang="en">Decide button basic background color and mouse hovered header menu color  (Input Hex value - ex: #888)</description>
			</var>

			<var name="footer_title01" type="text">
				<title xml:lang="ko">푸터 좌측 제목</title>
				<title xml:lang="en">Fotter left title</title>
			</var>

			<var name="footer_intro" type="textarea">
				<title xml:lang="ko">푸터 좌측 정보</title>
				<title xml:lang="en">Fotter left information</title>

				<description xml:lang="ko">html 사용 가능</description>
				<description xml:lang="en">html is available</description>
			</var>

			<var name="footer_title02" type="text">
				<title xml:lang="ko">푸터 중간 제목</title>
				<title xml:lang="en">Fotter middle title</title>
			</var>

			<var name="footer_info" type="textarea">
				<title xml:lang="ko">푸터 중간 정보</title>
				<title xml:lang="en">Fotter middle information</title>

				<description xml:lang="ko">html 사용 가능</description>
				<description xml:lang="en">html is available</description>
			</var>

			<var name="copyright" type="text">
				<title xml:lang="ko">카피라이트</title>
				<title xml:lang="en">Copyright</title>
			</var>
		</group>

		<!-- sec_01 -->
		<group>
			<title xml:lang="ko">섹션 1 - 슬라이드</title>
			<title xml:lang="en">Section 1 - Slider</title>

			<var name="sec01_use" type="select" default="Y">
				<title xml:lang="ko">섹션 1 사용</title>
				<title xml:lang="en">Use of section 1</title>

				<options value="Y">
					<title xml:lang="ko">사용함</title>
					<title xml:lang="en">Use</title>
				</options>

				<options value="N">
					<title xml:lang="ko">사용하지 않음</title>
					<title xml:lang="en">Not use</title>
				</options>
			</var>

			<var name="sli_pause" type="text">
				<title xml:lang="ko">슬라이드 변경 주기</title>
				<title xml:lang="en">Slide changing cycle</title>
				<description xml:lang="ko">슬라이드의 변경 주기를 입력하십시오. (1초 = 1000 / 숫자만 입력)</description>
				<description xml:lang="en">Setup changing cycle of slider. (1sec = 1000 / put in number only)</description>
			</var>

			<var name="sec01_img01" type="image">
				<title xml:lang="ko">슬라이드 1 이미지</title>
				<title xml:lang="en">slide 1 image</title>
				<description xml:lang="ko">1915*969 픽셀 사이즈 이미지 권장</description>
				<description xml:lang="en">Recommend 1915*969 sized image</description>
			</var>
			<var name="sec01_sub01" type="text">
				<title xml:lang="ko">슬라이드 1 부제목</title>
				<title xml:lang="en">slide 1 subtitle</title>
			</var>
			<var name="sec01_title01" type="text">
				<title xml:lang="ko">슬라이드 1 제목</title>
				<title xml:lang="en">slide 1 title</title>
				<description xml:lang="ko">span 태그로 감싸면 포인트컬러가 연동됩니다. (ex: &lt;span&gt;Neon&lt;span&gt; city)</description>
				<description xml:lang="en">Point color would be applied if cover with span tag</description>
			</var>
			<var name="sec01_desc01" type="textarea">
				<title xml:lang="ko">슬라이드 1 설명</title>
				<title xml:lang="en">slide 1 description</title>
			</var>
			<var name="sec01_link01" type="text">
				<title xml:lang="ko">슬라이드 1 버튼 링크</title>
				<title xml:lang="en">slide 1 button link</title>
			</var>

			<var name="sec01_img02" type="image">
				<title xml:lang="ko">슬라이드 2 이미지</title>
				<title xml:lang="en">slide 2 image</title>
				<description xml:lang="ko">1915*969 픽셀 사이즈 이미지 권장</description>
				<description xml:lang="en">Recommend 1915*969 sized image</description>
			</var>
			<var name="sec01_sub02" type="text">
				<title xml:lang="ko">슬라이드 2 부제목</title>
				<title xml:lang="en">slide 2 subtitle</title>
			</var>
			<var name="sec01_title02" type="text">
				<title xml:lang="ko">슬라이드 2 제목</title>
				<title xml:lang="en">slide 2 title</title>
				<description xml:lang="ko">span 태그로 감싸면 포인트컬러가 연동됩니다. (ex: &lt;span&gt;Neon&lt;span&gt; city)</description>
				<description xml:lang="en">Point color would be applied if cover with span tag</description>
			</var>
			<var name="sec01_desc02" type="textarea">
				<title xml:lang="ko">슬라이드 2 설명</title>
				<title xml:lang="en">slide 2 description</title>
			</var>
			<var name="sec01_link02" type="text">
				<title xml:lang="ko">슬라이드 2 버튼 링크</title>
				<title xml:lang="en">slide 2 button link</title>
			</var>

			<var name="sec01_img03" type="image">
				<title xml:lang="ko">슬라이드 3 이미지</title>
				<title xml:lang="en">slide 3 image</title>
				<description xml:lang="ko">1915*969 픽셀 사이즈 이미지 권장</description>
				<description xml:lang="en">Recommend 1915*969 sized image</description>
			</var>
			<var name="sec01_sub03" type="text">
				<title xml:lang="ko">슬라이드 3 부제목</title>
				<title xml:lang="en">slide 3 subtitle</title>
			</var>
			<var name="sec01_title03" type="text">
				<title xml:lang="ko">슬라이드 3 제목</title>
				<title xml:lang="en">slide 3 title</title>
				<description xml:lang="ko">span 태그로 감싸면 포인트컬러가 연동됩니다. (ex: &lt;span&gt;Neon&lt;span&gt; city)</description>
				<description xml:lang="en">Point color would be applied if cover with span tag</description>
			</var>
			<var name="sec01_desc03" type="textarea">
				<title xml:lang="ko">슬라이드 3 설명</title>
				<title xml:lang="en">slide 3 description</title>
			</var>
			<var name="sec01_link03" type="text">
				<title xml:lang="ko">슬라이드 3 버튼 링크</title>
				<title xml:lang="en">slide 3 button link</title>
			</var>

			<var name="sec01_img04" type="image">
				<title xml:lang="ko">슬라이드 4 이미지</title>
				<title xml:lang="en">slide 4 image</title>
				<description xml:lang="ko">1915*969 픽셀 사이즈 이미지 권장</description>
				<description xml:lang="en">Recommend 1915*969 sized image</description>
			</var>
			<var name="sec01_sub04" type="text">
				<title xml:lang="ko">슬라이드 4 부제목</title>
				<title xml:lang="en">slide 4 subtitle</title>
			</var>
			<var name="sec01_title04" type="text">
				<title xml:lang="ko">슬라이드 4 제목</title>
				<title xml:lang="en">slide 4 title</title>
				<description xml:lang="ko">span 태그로 감싸면 포인트컬러가 연동됩니다. (ex: &lt;span&gt;Neon&lt;span&gt; city)</description>
				<description xml:lang="en">Point color would be applied if cover with span tag</description>
			</var>
			<var name="sec01_desc04" type="textarea">
				<title xml:lang="ko">슬라이드 4 설명</title>
				<title xml:lang="en">slide 4 description</title>
			</var>
			<var name="sec01_link04" type="text">
				<title xml:lang="ko">슬라이드 4 버튼 링크</title>
				<title xml:lang="en">slide 4 button link</title>
			</var>

			<var name="sec01_img05" type="image">
				<title xml:lang="ko">슬라이드 5 이미지</title>
				<title xml:lang="en">slide 5 image</title>
				<description xml:lang="ko">1915*969 픽셀 사이즈 이미지 권장</description>
				<description xml:lang="en">Recommend 1915*969 sized image</description>
			</var>
			<var name="sec01_sub05" type="text">
				<title xml:lang="ko">슬라이드 5 부제목</title>
				<title xml:lang="en">slide 5 subtitle</title>
			</var>
			<var name="sec01_title05" type="text">
				<title xml:lang="ko">슬라이드 5 제목</title>
				<title xml:lang="en">slide 5 title</title>
				<description xml:lang="ko">span 태그로 감싸면 포인트컬러가 연동됩니다. (ex: &lt;span&gt;Neon&lt;span&gt; city)</description>
				<description xml:lang="en">Point color would be applied if cover with span tag</description>
			</var>
			<var name="sec01_desc05" type="textarea">
				<title xml:lang="ko">슬라이드 5 설명</title>
				<title xml:lang="en">slide 5 description</title>
			</var>
			<var name="sec01_link05" type="text">
				<title xml:lang="ko">슬라이드 5 버튼 링크</title>
				<title xml:lang="en">slide 5 button link</title>
			</var>
		</group>

		<!-- subpage -->
		<group>
			<title xml:lang="ko">서브페이지 레이아웃</title>
			<title xml:lang="en">Subpage layout</title>

			<var name="subpage_type" type="select" default="full">
				<title xml:lang="ko">서브페이지 레이아웃 타입</title>
				<title xml:lang="en">Subpage layout type</title>
				<options value="full">
					<title xml:lang="ko">풀 페이지</title>
					<title xml:lang="en">Full page</title>
				</options>
				<options value="left">
					<title xml:lang="ko">좌측 사이드 메뉴</title>
					<title xml:lang="en">Left side menu</title>
				</options>
				<options value="right">
					<title xml:lang="ko">우측 사이드 메뉴</title>
					<title xml:lang="en">Right side menu</title>
				</options>
			</var>

			<var name="subtop_height" type="text">
				<title xml:lang="ko">서브탑 최소 높이</title>
				<title xml:lang="en">Subtop area minimal height</title>
				<description xml:lang="ko">300 이상의 숫자를 입력하세요. (단위 : 픽셀)</description>
				<description xml:lang="en">Input number over 300. (unit : pixel)</description>
			</var>

			<var name="subtop_image" type="image">
				<title xml:lang="ko">서브탑 배경 이미지</title>
				<title xml:lang="en">Subtop background image</title>
				<description xml:lang="ko">설정된 서브탑 높이에 맞춘 이미지를 첨부하세요.</description>
				<description xml:lang="en">Attach image that matched height with subtop`s.</description>
			</var>

			<var name="subtop_color" type="text">
				<title xml:lang="ko">서브탑 배경색</title>
				<title xml:lang="en">Subtop background color</title>
				<description xml:lang="ko">ex : #242424</description>
				<description xml:lang="en">ex : #242424</description>
			</var>

			<var name="subtop_txt_use" type="select" default="Y">
				<title xml:lang="ko">서브탑 텍스트 사용</title>
				<title xml:lang="en">Use of subtop text</title>
				<options value="Y">
					<title xml:lang="ko">사용함</title>
					<title xml:lang="en">Use</title>
				</options>
				<options value="N">
					<title xml:lang="ko">사용하지 않음</title>
					<title xml:lang="en">Not use</title>
				</options>
			</var>

			<var name="subtop_txt_color" type="text">
				<title xml:lang="ko">서브탑 텍스트 컬러</title>
				<title xml:lang="en">Subtop text color</title>
			</var>

			<var name="subtop_title" type="text">
				<title xml:lang="ko">서브탑 제목</title>
				<title xml:lang="en">Subtop title</title>
			</var>

			<var name="subtop_subtitle" type="text">
				<title xml:lang="ko">서브탑 부제목</title>
				<title xml:lang="en">Subtop subtitle</title>
			</var>

			<var name="content_width" type="text">
				<title xml:lang="ko">컨텐츠 너비</title>
				<title xml:lang="en">Content width</title>
				<description xml:lang="ko">컨텐츠 영역의 최대 너비를 입력하세요. (숫자만 입력 / 단위 : px / 사이드 메뉴 사용 시 사이드 메뉴 포함 너비)</description>
				<description xml:lang="en">Input width value of content area (Only number / unit : px / include side menu area if it used)</description>
			</var>
		</group>
	</extra_vars>


	<menus>
		<menu name="GNB" maxdepth="2" default="true">
			<title xml:lang="ko">메인 메뉴</title>
			<title xml:lang="en">Main menu</title>
			<title xml:lang="jp">Main menu</title>
			<title xml:lang="zh-CN">Main menu</title>
		</menu>
		<menu name="FNB" maxdepth="2" default="true">
			<title xml:lang="ko">푸터 메뉴</title>
			<title xml:lang="en">Main menu</title>
			<title xml:lang="jp">Main menu</title>
			<title xml:lang="zh-CN">Main menu</title>
		</menu>
	</menus>
</layout>